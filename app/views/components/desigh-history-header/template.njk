<header class="govuk-header app-header app-header--design-history" role="banner" data-module="govuk-header">
  <div class="govuk-header__container govuk-width-container app-header__container">
    <div class="govuk-header__logo app-header__logo">
      <span class="govuk-header__logotype">
        <span class="govuk-header__logotype-text">
           <span class="govuk-caption-m app-!-inherit-colour">Design History</span>
           <a href="{{ params.homepageUrl | default("/") }}" class="govuk-header__link govuk-header__link--homepage">{{ params.designHistoryName }}</a>
        </span>
      </span>
        {# <span class="govuk-header__product-name">
          {{ params.designHistoryName }}
        </span> #}
      </a>
    </div>
  </div>
</header>
